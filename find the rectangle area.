/* **********************************include sec start******************************************** */
#include <stdio.h>
#include <stdlib.h>
#include <math.h>
/* **********************************include sec end******************************************** */


int main()
{
    int num1,num2,area;
    printf("Enter length:  \n");
    scanf("%d", &num1);
    printf("Enter width: ");
    scanf("%d",&num2);
    area=(num1*num2);
    printf("Area of rectangle = %d ",area);
    printf("sq. units");
    return 0;
}

